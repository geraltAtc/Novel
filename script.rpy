    with fade
    
    show boichik1 at left2
    with dissolve
    
    show blond5 at right2
    with dissolve
    
    menu:
        "Молча встать":
            ma "Я уже думала до вечера не проснешься"
        "Поинтересоваться сколько времени":
            m "Маш, не подскажешь время?"
            ma "9 утра"
            ma "Ну ты и поспал конечно"
            m "Да..."
        "Сказать Доброе утро":
            m "Доброе утро, Маша"
